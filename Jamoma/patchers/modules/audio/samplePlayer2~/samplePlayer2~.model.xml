<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="audio/gain' type='decimal">100.000000</command>
    <command address="audio/mute' type='boolean">0</command>
    <command address="loop' type='boolean">0</command>
    <command address="loopsync/active' type='boolean">0</command>
    <command address="loopsync/mode' type='string">loop_time</command>
    <command address="reverse' type='boolean">0</command>
    <command address="sample/start' type='decimal">0</command>
    <command address="sample/stop' type='decimal">0</command>
    <command address="sample/vzoom' type='decimal">1.</command>
    <command address="speed/active' type='boolean">0</command>
    <command address="speed/ratio' type='decimal">1.000000</command>  
    <command address="pitch/ratio' type='decimal">0.0000</command>  
    <command address="pitch/active' type='boolean">0</command>
    <command address="view/freeze' type='boolean">0</command>
  </preset>
</jamoma>
