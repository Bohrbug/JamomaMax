<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="angle' type='integer">0</command>
    <command address="boundmode' type='string">clip</command>
    <command address="mirror' type='boolean">1</command>
    <command address="offsetX' type='decimal">0.000000</command>
    <command address="offsetY' type='decimal">0.000000</command>
    <command address="rotate1' type='boolean">0</command>
    <command address="rotate2' type='boolean">0</command>
    <command address="video/bypass' type='boolean">0</command>
    <command address="video/freeze' type='boolean">0</command>
    <command address="video/mute' type='boolean">0</command>
    <command address="video/preview' type='boolean">0</command>
    <command address="view/freeze' type='boolean">0</command>

    <command address="zoomX' type='decimal">1.000000</command>
    <command address="zoomY' type='decimal">1.000000</command>
  </preset>
</jamoma>
