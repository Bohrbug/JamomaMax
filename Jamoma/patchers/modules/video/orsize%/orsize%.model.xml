<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="angle">0</command>
    <command address="boundmode">clip</command>
    <command address="mirror">1</command>
    <command address="offsetX">0.000000</command>
    <command address="offsetY">0.000000</command>
    <command address="rotate1">0</command>
    <command address="rotate2">0</command>
    <command address="video/bypass">0</command>
    <command address="video/freeze">0</command>
    <command address="video/mute">0</command>
    <command address="video/preview">0</command>
    <command address="view/freeze">0</command>

    <command address="zoomX">1.000000</command>
    <command address="zoomY">1.000000</command>
  </preset>
</jamoma>
