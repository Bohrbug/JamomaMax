<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="colormode' type='string">wrap</command>
    <command address="gang' type='boolean">1</command>
    <command address="mode' type='integer">0</command>
    <command address="scale' type='integer">8</command>
    <command address="video/bypass' type='boolean">0</command>
    <command address="video/freeze' type='boolean">0</command>
    <command address="video/mute' type='boolean">0</command>
    <command address="video/preview' type='boolean">0</command>
    <command address="view/freeze' type='boolean">0</command>

    <command address="x/range' type='integer">25</command>
    <command address="x/step' type='integer">30</command>
    <command address="y/range' type='integer">25</command>
    <command address="y/step' type='integer">30</command>
  </preset>
</jamoma>
