<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="colormode">wrap</command>
    <command address="gang">1</command>
    <command address="mode">0</command>
    <command address="scale">8</command>
    <command address="video/bypass">0</command>
    <command address="video/freeze">0</command>
    <command address="video/mute">0</command>
    <command address="video/preview">0</command>
    <command address="view/freeze">0</command>

    <command address="x/range">25</command>
    <command address="x/step">30</command>
    <command address="y/range">25</command>
    <command address="y/step">30</command>
  </preset>
</jamoma>
