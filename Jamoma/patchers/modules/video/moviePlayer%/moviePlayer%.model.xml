<?xml version='1.0' encoding='iso-8859-1' standalone='yes' ?>

<jamoma version='0.6'
		xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
		xsi:schemaLocation='http://jamoma.org/ file:jamoma.xsd'
>
  <preset name='default'>
    <command address="colormode' type='string">argb</command>
    <command address="deinterlace' type='boolean">1</command>
    <command address="dim' type='array">320.000000 240.000000 </command>
    <command address="highquality' type='boolean">1</command>
    <command address="loop' type='string">normal</command>
    <command address="singlefield' type='boolean">0</command>
    <command address="view/freeze' type='boolean">0</command>
    <command address="unique' type='boolean">0</command>
    <command address="video/bypass' type='boolean">0</command>
    <command address="video/freeze' type='boolean">0</command>
    <command address="video/mute' type='boolean">0</command>
    <command address="video/preview' type='boolean">0</command>
  </preset>
  <preset number='2' name='320x240'>
    <command address="dim' type='array">320.000000 240.000000 </command>
  </preset>
  <preset number='3' name='640x480 (NTSC)'>
    <command address="dim' type='array">640.000000 320.000000 </command>
  </preset>
  <preset number='4' name='720 480 (PAL)'>
    <command address="dim' type='array">720.000000 480.000000 </command>
  </preset>
  <preset number='5' name='Colormode ARGB'>
    <command address="colormode' type='string">argb</command>
  </preset>
  <preset number='6' name='Colormode uyvy'>
    <command address="colormode' type='string">uyvy</command>
  </preset>
</jamoma>
